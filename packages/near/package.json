{
  "name": "@textile/near-storage",
  "version": "6.0.2",
  "description": "NEAR Protocol provider implementation for Textile's Filecoin bridge system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./registry": {
      "require": "./dist/cjs/registry.js",
      "import": "./dist/esm/registry.js"
    },
    "./provider": {
      "require": "./dist/cjs/provider.js",
      "import": "./dist/esm/provider.js"
    },
    "./token": {
      "require": "./dist/cjs/token.js",
      "import": "./dist/esm/token.js"
    }
  },
  "files": [
    "dist/*.js?(.map)",
    "dist/*.d.ts",
    "src/*.ts"
  ],
  "dependencies": {
    "@types/bs58": "^4.0.1",
    "bs58": "^4.0.1",
    "near-api-js": "^0.42.0"
  },
  "devDependencies": {
    "localstorage-memory": "^1.0.3"
  }
}